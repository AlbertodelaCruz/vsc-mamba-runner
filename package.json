{
  "name": "mamba-runner",
  "displayName": "Mamba Runner",
  "description": "",
  "version": "0.0.3",
  "publisher": "acgri82",
  "repository": {
    "type": "git",
    "url": "git://github.com/albertodelacruz/vsc-mamba-runner.git"
  },
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:mamba.runTest",
    "onCommand:mamba.runUnitTests"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mamba.runTest",
        "title": "Mamba Run Test"
      },
      {
        "command": "mamba.runUnitTests",
        "title": "Mamba Run Unit Tests"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "17.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.34.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "typescript": "^4.5.5"
  }
}
